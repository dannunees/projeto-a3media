                <script type="text/javascript">
                    jQuery(document).ready(function () {
                        const pitchTime = 3249;

                        $("#ready").hide();
                        $("#watch").show();
                        $("#watch_mobile").show();

                        const interval = setInterval(() => {
                            const videoTime = localStorage.getItem(
                                "67864fb06e451dbbada28cc2",
                            );

                            if (Number(videoTime) >= pitchTime) {
                                jQuery("#acces").fadeIn();
                                countDownStart();
                                $("#watch").hide();
                                $("#watch_mobile").hide();
                                $("#ready").show();

                                setTimeout(() => {
                                    $("html, body").animate(
                                        {
                                            scrollTop:
                                                $("#productarea").offset().top,
                                        },
                                        1000,
                                    );
                                }, 1000);

                                clearInterval(interval);
                            }
                        }, 1000);

                        window.showEls = () => {
                            jQuery("#acces").fadeIn();
                            countDownStart();
                            $("#watch_mobile").hide();
                            $("#watch").hide();
                            $("#ready").show();

                            setTimeout(() => {
                                $("html, body").animate(
                                    {
                                        scrollTop:
                                            $("#productarea").offset().top,
                                    },
                                    1000,
                                );
                            }, 1000);
                        };

                        function isMobileDevice() {
                            return /Mobi|Android/i.test(navigator.userAgent);
                        }

                        function startTimer(duration, display) {
                            var timer = duration;

                            setInterval(function () {
                                display.textContent = get_time_string(timer);

                                if (timer > 0) {
                                    timer--;
                                }

                                if (typeof localStorage != "undefined") {
                                    localStorage.setItem(
                                        "reservation_countdown",
                                        timer,
                                    );
                                }
                            }, 1000); // Changed to 1000ms for accurate 1-second intervals
                        }

                        function countDownStart() {
                            if ($("#counter").length) {
                                var timer = isMobileDevice() ? 1200 : 1800; // 20 minutes for mobile, 30 minutes otherwise
                                if (isNaN(timer)) timer = 1800;
                                if (timer < 0) timer = 0;

                                $("#counter").html(get_time_string(timer));
                                $("#counter1").html(get_time_string(timer)); // Corrected to .html() for display
                                $("#counter2").html(get_time_string(timer)); // Corrected to .html() for display

                                startTimer(timer, $("#counter").get(0));
                                startTimer(timer, $("#counter1").get(0));
                                startTimer(timer, $("#counter2").get(0));
                            }
                        }

                        function get_time_string(timer) {
                            var minutes, seconds;
                            minutes = parseInt(timer / 60, 10);
                            seconds = parseInt(timer % 60, 10);

                            minutes = minutes < 10 ? "0" + minutes : minutes;
                            seconds = seconds < 10 ? "0" + seconds : seconds;

                            return minutes + ":" + seconds;
                        }
                    });
                </script>
